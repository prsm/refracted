{
  "version": "0.0.1-8",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "cross-env NODE_ENV=production tsdx build",
    "test": "tsdx test --passWithNoTests",
    "test:watch": "tsdx test --watch --passWithNoTests",
    "lint": "eslint '*/**/*.{ts,tsx}' --quiet",
    "format": "prettier --write '*/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix -f git-log",
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}/": [
      "eslint --fix",
      "git add"
    ],
    "*.{ts,css,md}": "prettier --write"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@pr1sm/refracted",
  "description": "The React UI library for PR1SM",
  "keywords": [
    "react",
    "ui",
    "framer-motion",
    "tailwindcss",
    "storybook"
  ],
  "author": "Jonas Schultheiss <jonas.schultheiss@pr1sm.gg> (https://jonasschultheiss.dev)",
  "homepage": "https://github.com/prsm/refracted",
  "bugs": {
    "url": "https://github.com/prsm/refracted/issues"
  },
  "private": false,
  "module": "dist/refracted.esm.js",
  "size-limit": [
    {
      "path": "dist/refracted.cjs.production.min.js",
      "limit": "100 KB"
    },
    {
      "path": "dist/refracted.esm.js",
      "limit": "100 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@size-limit/preset-small-lib": "^5.0.2",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.6",
    "@storybook/theming": "^6.3.7",
    "@svgr/rollup": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "chromatic": "^5.9.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-tailwindcss": "^1.14.2",
    "eslint-plugin-unicorn": "^35.0.0",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^11.1.2",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.1",
    "size-limit": "^5.0.2",
    "storybook-formik": "^2.1.6",
    "storybook-tailwind-dark-mode": "^1.0.9",
    "tailwindcss": "^2.2.7",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.4",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prsm/refracted.git"
  }
}
