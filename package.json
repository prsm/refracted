{
  "version": "0.0.1-3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "yarn": "^1.22.0"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "NODE_ENV=production tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@pr1sm/refracted",
  "description": "The React UI library for PR1SM",
  "keywords": [
    "react",
    "ui",
    "framer-motion",
    "tailwindcss",
    "storybook"
  ],
  "author": "Jonas Schultheiss <jonas.schultheiss@pr1sm.gg> (https://jonasschultheiss.dev)",
  "homepage": "https://github.com/prsm/refracted",
  "bugs": {
    "url": "https://github.com/prsm/refracted/issues"
  },
  "private": false,
  "module": "dist/refracted.esm.js",
  "size-limit": [
    {
      "path": "dist/refracted.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/refracted.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@size-limit/preset-small-lib": "^5.0.2",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.12",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "husky": "^7.0.1",
    "identity-obj-proxy": "^3.0.0",
    "postcss": "^8.3.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rollup-plugin-postcss": "^4.0.1",
    "size-limit": "^5.0.2",
    "storybook-tailwind-dark-mode": "^1.0.9",
    "tailwindcss": "^2.2.7",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "eslint": "7.17.0"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prsm/refracted.git"
  }
}
